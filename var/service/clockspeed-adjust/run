#!/bin/sh
# generated by clockspeed-conf-0.4.5_0
# $Header:  Exp $

# net-misc/clockspeed supervise script

[ -f /etc/profile ] && source /etc/profile

exec 2>&1
while [ ! -p "/var/lib/clockspeed/adjust" ]; do
  echo "Waiting for /var/lib/clockspeed/adjust"
  sleep 10
done
chgrp clockspeed  "/var/lib/clockspeed/adjust"
chmod 0720  "/var/lib/clockspeed/adjust"
exec setuidgid clockspeed envdir ./env ./run-adjust
#.

